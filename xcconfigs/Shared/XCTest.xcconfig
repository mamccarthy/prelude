ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES

DEBUG_INFORMATION_FORMAT = dwarf

APPLICATION_EXTENSION_API_ONLY = NO

WRAPPER_EXTENSION = xctest

LD_RUNPATH_SEARCH_PATHS[sdk=iphone*] = @loader_path/Frameworks
